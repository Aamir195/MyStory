import 'package:flutter/material.dart';
import 'package:practise/app_screen/home.dart';
import 'package:practise/app_screen/login_page.dart';
import 'package:practise/util/Constants.dart';
import 'package:shared_preferences/shared_preferences.dart';

import 'app_screen/login_page.dart';

Future main() async {
  WidgetsFlutterBinding.ensureInitialized();

  Constants.pref = await SharedPreferences.getInstance();

  runApp(
    MaterialApp(
      title: "My Practice App",
      home: LoginPage(),
      theme: ThemeData(
        primarySwatch: Colors.green,
      ),
      routes: {
        "/home": (context) => HomePage(),
        "/login_page": (contex) => LoginPage()
      },
    ),
  );
}
